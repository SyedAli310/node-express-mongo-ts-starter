{
  "name": "express-mongo-ts-starter",
  "version": "1.0.0",
  "description": "This is a started template for Node + Express + MongoDB backend server using TypeScript",
  "main": "src/server.ts",
  "scripts": {
    "start": "node dist/src/server.js",
    "serve:dev": "nodemon src/server.ts",
    "debug": "set NODE_OPTIONS=--inspect && nodemon src/server.ts",
    "dev": "ts-node src/server.ts",
    "migration:run": "ts-node ./scripts/run-migration.ts",
    "build": "tsc",
    "build:prod": "npm install && npm run build && npm run build:email-templates",
    "build:email-templates": "copyfiles -u 2 \"src/email-templates/**/*.{html,txt,mjml}\" dist/src/email-templates",
    "vercel-build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SyedAli310/node-express-mongo-ts-starter.git"
  },
  "keywords": [],
  "author": "Syed Ali (GitHub: SyedAli310)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SyedAli310/node-express-mongo-ts-starter/issues"
  },
  "homepage": "https://github.com/SyedAli310/node-express-mongo-ts-starter#readme",
    "dependencies": {
    "bcryptjs": "^3.0.2",
    "cli-color": "^2.0.4",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "figlet": "^1.8.2",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "moment": "^2.30.1",
    "mongoose": "^8.16.4",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/cli-color": "^2.0.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/figlet": "^1.7.0",
    "@types/node": "^24.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ua-parser-js": "^0.7.39",
    "axios": "^1.10.0",
    "copyfiles": "^2.4.1",
    "migrate-mongo": "^12.1.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  }
}
